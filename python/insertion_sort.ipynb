      "source": [
        "def insertion_sort(arr):\n",
        "\n",
        "  for i in range(1, len(arr)):\n",
        "\n",
        "    key = arr[i]\n",
        "\n",
        "    j = i - 1\n",
        "\n",
        "    #Insert arr[i] into the sorted list arr[0:j-1] at the correct place\n",
        "    while j >= 0 and arr[j] > key:\n",
        "      arr[j+1] = arr[j]\n",
        "      j -= 1\n",
        "\n",
        "    arr[j+1] = key\n",
        "\n"
      ]
    }
  ]
}