        "\n",
        "  visited.add(v)\n",
        "  print(v)\n",
        "  \n",
        "  for neighbor in G[v]:\n",
        "    if neighbor not in visited:\n",
        "      DFS_helper(G, neighbor, visited)\n",
        "\n",
        "def DFS(G, v):\n",
        "\n",
        "  visited = set()\n",
        "  DFS_helper(G, v, visited)\n"
      ]
